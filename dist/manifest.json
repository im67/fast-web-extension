{
  "manifest_version": 3,
  "version": "0.0.1",
  "name": "Mario Next For V3",
  "description": "A chrome extension for sgfe",
  "options_page": "index.html",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "webRequest",
    "storage",
    "contextMenus",
    "bookmarks"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src http://127.0.0.1:8000 'self'; object-src 'none'"
  }
}